#Download and install apache2 in ubuntu instance.#
sudo apt update
sudo apt install apache2 wget unzip -y
wget https://www.tooplate.com/zip-templates/2128_tween_agency.zip
unzip 2128_tween_agency.zip
sudo cp -r 2128_tween_agency/* /var/www/html/  
sudo systemctl restart apache2
#open http://youripadress 
#You should be able to see the website up and running.
